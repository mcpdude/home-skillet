{
  "name": "home-skillet-backend",
  "version": "1.0.0",
  "description": "Backend API for Home Skillet property management application",
  "main": "backend/src/server.js",
  "scripts": {
    "start": "node backend/src/server.js",
    "dev": "nodemon backend/src/server.js",
